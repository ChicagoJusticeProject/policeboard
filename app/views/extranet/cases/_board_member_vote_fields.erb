<div class="fields board-member-vote-fields board-member-vote">
	<%= f.hidden_field :_destroy %>
	<%= link_to "#", class: "remove_fields" do %>
		<i class="fa fa-times"></i>x
	<% end %>
	<%= f.object.board_member.full_name %>
	
	<%= f.collection_radio_buttons :vote_id, Vote.all, :id, :name %>
	
	<div class="dissent-detail">
		<p><%= f.label :dissent_description, "Reason for dissent" %></p>
		<%= f.text_area :dissent_description, class: "dissent-description" %>
	</div>	
</div>